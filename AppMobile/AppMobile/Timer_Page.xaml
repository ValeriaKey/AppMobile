<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMobile.Timer_Page">
    <ContentPage.Content>
        <StackLayout>
            <FlexLayout>
                <Label x:Name="lbl"
                       Text="Valjuta nuppule ja siis"
                    VerticalOptions="CenterAndExpand"
                       FontSize="Medium"
                       BackgroundColor="Beige">
                    <Label.GestureRecognizers>
                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped"/>
                    </Label.GestureRecognizers>
                </Label>
                <Button x:Name="timer_btn"
                    Text="Naite info"
                    FontSize="Large"
                    Clicked="timer_btn_Clicked">
                </Button>
            </FlexLayout>
            <Button x:Name="tagasi"
                    Text="Tagasi"
                    FontSize="Large">
                
            </Button>
                    
        </StackLayout>
    </ContentPage.Content>
</ContentPage>